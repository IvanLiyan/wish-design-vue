{
  "props": [
    {
      "name": "width",
      "desc": "$t('datepicker.api.width')",
      "type": "Number",
      "optionalValue": "",
      "default": "100%",
      "version": ""
    },
    {
      "name": "size",
      "desc": "$t('datepicker.api.size')",
      "type": "string",
      "optionalValue": ["small", "(None)", "large"],
      "default": "(None)",
      "version": ""
    },
    {
      "name": "type",
      "desc": "$t('datepicker.api.type')",
      "type": "string",
      "optionalValue": ["date", "daterange", "datetimerange", "year", "month", "monthrange"],
      "default": "date",
      "version": ""
    },
    {
      "name": "format",
      "desc": "$t('datepicker.api.format')",
      "type": "string",
      "optionalValue": "",
      "default": "<strong>date、daterange</strong>: <wbr/>yyyy-MM-dd; <br /> <strong>datetime、datetimerange</strong>: <wbr/>yyyy-MM-dd HH:mm:ss; <br /> <strong>year</strong>: <wbr/>yyyy; <br /><strong>month</strong>: <wbr/>yyyy-MM",
      "version": ""
    },
    {
      "name": "value-format",
      "desc": "$t('datepicker.api.value-format')",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "label",
      "desc": "$t('datepicker.api.label')",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "readonly",
      "desc": "$t('datepicker.api.readonly')",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "disabled",
      "desc": "$t('datepicker.api.disabled')",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "multiple",
      "desc": "$t('datepicker.api.multiple')",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "placement",
      "desc": "$t('datepicker.api.placement')",
      "type": "string",
      "optionalValue": [
        "top",
        "top-start",
        "top-end",
        "bottom",
        "bottom-start",
        "bottom-end",
        "left",
        "left-start",
        "left-end",
        "right",
        "right-start",
        "right-end"
      ],
      "default": "bottom-start",
      "version": ""
    },
    {
      "name": "name",
      "desc": "$t('datepicker.api.name')",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "value",
      "desc": "$t('datepicker.api.value')",
      "type": ["Date", "Array", "string"],
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "placeholder",
      "desc": "$t('datepicker.api.placeholder')",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "popper-class",
      "desc": "$t('datepicker.api.popper-class')",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "append-to-container",
      "desc": "$t('datepicker.api.append-to-container')",
      "type": "boolean",
      "optionalValue": "",
      "default": "true",
      "version": ""
    },
    {
      "name": "get-popup-container",
      "desc": "$t('datepicker.api.get-popup-container')",
      "type": "function():HTMLElement",
      "optionalValue": "",
      "default": "default callback document.body",
      "version": ""
    },
    {
      "name": "popper-options",
      "desc": "$t('datepicker.api.popper-options')",
      "type": "object",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "week-start",
      "desc": "$t('datepicker.api.week-start')",
      "type": "number",
      "optionalValue": "0 ~ 6",
      "default": "1",
      "version": ""
    }
  ],
  "events": [
    {
      "name": "update:open",
      "desc": "$t('datepicker.api.update:open')",
      "signature": "function (open: boolean)",
      "args": ""
    },
    {
      "name": "input",
      "desc": "$t('datepicker.api.input')",
      "signature": "function (value: date / date[])",
      "args": ""
    },
    {
      "name": "pick-range",
      "desc": "$t('datepicker.api.pick-range')",
      "signature": "(selection: { from: Date, selecting: boolean, to?: Date })",
      "args": "The parameter from indicates the start time, 'selecting' indicates whether the machine is still being selected, and 'to' indicates the end time, it can be null."
    }
  ]
}
