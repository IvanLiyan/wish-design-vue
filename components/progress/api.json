{
  "props": [
    {
      "name": "percentage",
      "desc": "$t('progress.api.percentage')",
      "type": "number",
      "optionalValue": "when type = 'line', >=0; when type = 'circle', [0, 100]",
      "default": "0",
      "version": ""
    },
    {
      "name": "status",
      "desc": "$t('progress.api.status')",
      "type": "string",
      "optionalValue": ["success", "error"],
      "default": "",
      "version": ""
    },
    {
      "name": "type",
      "desc": "$t('progress.api.type')",
      "type": "string",
      "optionalValue": ["line", "circle"],
      "default": "line",
      "version": ""
    },
    {
      "name": "color",
      "desc": "$t('progress.api.color')",
      "type": "string",
      "optionalValue": "",
      "default": "#305BEF",
      "version": ""
    },
    {
      "name": "stroke-width",
      "desc": "$t('progress.api.stroke-width')",
      "type": "string",
      "optionalValue": "",
      "default": "line: 8 / circle: 5",
      "version": ""
    },
    {
      "name": "width",
      "desc": "$t('progress.api.width')",
      "type": "number",
      "optionalValue": "",
      "default": "72",
      "version": ""
    }
  ]
}
