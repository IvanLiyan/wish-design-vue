{
  "props": [
    {
      "name": "unborder",
      "desc": "$t('pagination.api.unborder')",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "current-page",
      "desc": "$t('pagination.api.current-page')",
      "type": "number",
      "optionalValue": "",
      "default": "1",
      "version": ""
    },
    {
      "name": "page-size",
      "desc": "$t('pagination.api.page-size')",
      "type": "number",
      "optionalValue": "",
      "default": "10",
      "version": ""
    },
    {
      "name": "page-size-options",
      "desc": "$t('pagination.api.page-size-options')",
      "type": "number[]",
      "optionalValue": "",
      "default": "[10, 20, 50, 100]",
      "version": ""
    },
    {
      "name": "show-size-changer",
      "desc": "$t('pagination.api.show-size-changer')",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "total",
      "desc": "$t('pagination.api.total')",
      "type": "number",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "show-total",
      "desc": "$t('pagination.api.show-total')",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "pager-count",
      "desc": "$t('pagination.api.pager-count')",
      "type": "number",
      "optionalValue": "An odd number greater than or equal to 5 and less than or equal to 21",
      "default": "7",
      "version": ""
    },
    {
      "name": "show-quick-jumper",
      "desc": "$t('pagination.api.show-quick-jumper')",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    }
  ],
  "events": [{
    "name": "change",
    "desc": "$t('pagination.api.change')",
    "args": "Function(current, size)"
  }]
}
