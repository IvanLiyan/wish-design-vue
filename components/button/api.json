{
  "props": [
    {
      "name": "type",
      "desc": "$t('button.api.type')",
      "type": "string",
      "optionalValue": [
        "primary",
        "secondary",
        "third",
        "ghost",
        "text",
        "link",
        "dashed",
        "success",
        "warning",
        "error"
      ],
      "default": "primary",
      "version": ""
    },
    {
      "name": "loading",
      "desc": "$t('button.api.loading')",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "disabled",
      "desc": "$t('button.api.disabled')",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "icon",
      "desc": "$t('button.api.icon')",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "href",
      "desc": "$t('button.api.href')",

      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "to",
      "desc": "$t('button.api.to')",
      "type": ["string", "object"],
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "link-size",
      "desc": "$t('button.api.link-size')",
      "type": "string",
      "optionalValue": ["normal", "small"],
      "default": "normal",
      "version": ""
    },
    {
      "name": "html-type",
      "desc": "$t('button.api.html-type')",
      "type": "string",
      "optionalValue": ["button", "submit", "reset"],
      "default": "button",
      "version": ""
    },
    {
      "name": "target",
      "desc": "$t('button.api.target')",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    }
  ],
  "events": [
    {
      "name": "click",
      "desc": "$t('button.api.click')",
      "signature": "function (event: Event)",
      "args": "(event: Event)"
    }
  ]
}
