{
  "props": [{
    "name": "action",
    "desc": "必选参数，上传的地址",
    "type": "string",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "headers",
    "desc": "设置上传的请求头部",
    "type": "object",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "multiple",
    "desc": "是否支持多选文件(只在list-type为text有效)",
    "type": "boolean",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "data",
    "desc": "上传时附带的额外参数",
    "type": "object",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "name",
    "desc": "上传的文件字段名",
    "type": "string",
    "optionalValue": "",
    "default": "file",
    "version": ""
  },
  {
    "name": "with-credentials",
    "desc": "支持发送 cookie 凭证信息",
    "type": "boolean",
    "optionalValue": "",
    "default": "false",
    "version": ""
  },
  {
    "name": "show-file-list",
    "desc": "是否显示已上传文件列表(只在list-type为text有效)",
    "type": "boolean",
    "optionalValue": "",
    "default": "true",
    "version": ""
  },
  {
    "name": "accept",
    "desc": "接受上传的 <a href='https://developer.mozilla.org/en-US/docs/Web/HTML/Attributes/accept' target='_blank'>文件类型</a>（thumbnail-mode 模式下此参数无效）, <strong>请不要依赖该属性来做校验，必须要在服务端进行校验</strong>",
    "type": "string",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "on-remove",
    "desc": "文件列表移除文件时的钩子",
    "type": "function(file, fileList)",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "on-success",
    "desc": "文件上传成功时的钩子",
    "type": "function(response, file, fileList)",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "on-error",
    "desc": "文件上传失败时的钩子",
    "type": "function(err, file, fileList)",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "on-progress",
    "desc": "文件上传时的钩子",
    "type": "function(event, file, fileList)",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "on-change",
    "desc": "文件状态改变时的钩子，添加文件、上传成功和上传失败时都会被调用",
    "type": "function(file, fileList)",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "before-upload",
    "desc": "上传文件之前的钩子，参数为上传的文件，若返回 false 或者返回 Promise 且被 reject，则停止上传。",
    "type": "function(file)",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "before-remove",
    "desc": "删除文件之前的钩子，参数为上传的文件和文件列表，若返回 false 或者返回 Promise 且被 reject，则停止上传。",
    "type": "function(file, fileList)",
    "optionalValue": "",
    "default": "",
    "version": ""
  },
  {
    "name": "list-type",
    "desc": "文件列表的类型",
    "type": "string",
    "optionalValue": "text / picture-list / picture-card",
    "default": "text",
    "version": ""
  },
  {
    "name": "file-list",
    "desc": "上传的文件列表, 例如: [{ name: 'food.jpg', url: 'https://xxx.cdn.com/xxx.jpg' }]",
    "type": "Array",
    "optionalValue": "",
    "default": "[]",
    "version": ""
  },
  {
    "name": "disabled",
    "desc": "是否禁用",
    "type": "boolean",
    "optionalValue": "",
    "default": "false",
    "version": ""
  },
  {
    "name": "limit",
    "desc": "最大允许上传个数",
    "type": "number",
    "optionalValue": "",
    "default": "",
    "version": ""
  }],
  "slots": [{
    "name": "trigger",
    "desc": "触发文件选择框的内容"
  },
  {
    "name": "tip",
    "desc": "提示说明文字"
  }],
  "methods": [{
    "name": "clearFiles()",
    "desc": "清空已上传的文件列表（该方法不支持在 before-upload 中调用）",
    "args": ""
  },
  {
    "name": "abort(file: fileList)",
    "desc": "取消上传请求",
    "args": ""
  },
  {
    "name": "submit()",
    "desc": "手动上传文件列表",
    "args": ""
  }]
}
