{
  "props": [
    {
      "name": "type",
      "desc": "$t('input.api.type')",
      "type": "string",
      "optionalValue": ["-", "line", "textarea"],
      "default": "-",
      "version": ""
    },

    {
      "name": "clearable",
      "desc": "$t('input.api.clearable')",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "invalid",
      "desc": "$t('input.api.invalid')",
      "type": "boolean",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "disabled",
      "desc": "$t('input.api.disabled')",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "max-length",
      "desc": "$t('input.api.max-length')",
      "type": "Number",
      "optionalValue": "",
      "default": "100",
      "version": ""
    },

    {
      "name": "value(v-model)",
      "desc": "$t('input.api.value')",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "autosize",
      "desc": "$t('input.api.autosize')",
      "type": ["boolean", "object"],
      "optionalValue": "",
      "default": "{ minRows: 2, maxRows: 6 }",
      "version": ""
    }
  ],
  "slots": [
    {
      "name": "prefix",
      "desc": "$t('input.api.prefix')"
    },
    {
      "name": "suffix",
      "desc": "$t('input.api.suffix')"
    }
  ],
  "events": [
    {
      "name": "input",
      "desc": "$t('input.api.input')",
      "args": "(value: string)"
    },
    {
      "name": "change",
      "desc": "$t('input.api.change')",
      "args": "(value: string)"
    },
    {
      "name": "clear",

      "desc": "$t('input.api.clear')",
      "args": ""
    },
    {
      "name": "enter",
      "desc": "$t('input.api.enter')",
      "args": "(event: Event)",
      "version": "0.3.5"
    }
  ]
}
