{
  "params": [
    {
      "name": "type",
      "desc": "消息类型，用于显示图标",
      "type": "string",
      "optionalValue": ["default", "success", "info", "warning", "error"],
      "default": "default",
      "version": ""
    },
    {
      "name": "title",
      "desc": "Confirm 标题",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "message",
      "desc": "Confirm 消息正文内容",
      "type": ["string", "VNode"],
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "closable",
      "desc": "是否显示关闭按钮，同时是否支持 esc 关闭",
      "type": "boolean",
      "optionalValue": "",
      "default": "true",
      "version": ""
    },
    {
      "name": "maskClosable",
      "desc": "点击蒙层是否允许关闭",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "okButtonText",
      "desc": "确定按钮的文本内容",
      "type": "string",
      "optionalValue": "",
      "default": "确定",
      "version": ""
    },
    {
      "name": "showOkButton",
      "desc": "是否显示确定按钮",
      "type": "boolean",
      "optionalValue": "",
      "default": "true",
      "version": ""
    },
    {
      "name": "cancelButtonText",
      "desc": "取消按钮的文本内容",
      "type": "string",
      "optionalValue": "",
      "default": "取消",
      "version": ""
    },
    {
      "name": "showCancelButton",
      "desc": "是否显示取消按钮",
      "type": "boolean",
      "optionalValue": "",
      "default": "default为true，其他为false",
      "version": ""
    },
    {
      "name": "width",
      "desc": "宽度 (当为 string 时需要带上单位)",
      "type": ["string", "number"],
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "onOk",
      "desc": "点击确认按钮后的回调, 支持返回 Promise, 返回action类型confirm",
      "type": "function",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "onCancel",
      "desc": "点击取消按钮或关闭按钮后的回调, 支持返回 Promise, 返回action类型cancel或close",
      "type": "function",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "useHTMLString",
      "desc": "是否将 message 属性作为 HTML 片段处理",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    }
  ]
}
