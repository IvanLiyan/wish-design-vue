{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx",
    "transform-runtime",
    "transform-object-rest-spread",
    "syntax-object-rest-spread"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [["istanbul", {
        "exclude": [
          "components/navbar/**/*",
          "**/doc/*",
          "**/tests/*",
          "test/**/*"
        ]
      }]]
    },
    "utils": {
      "presets": [
        ["env", {
          "targets": {
            "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
          }
        }],
        "stage-2"
      ],
      "plugins": [
        ["module-resolver", {
          "root": ["@wish/wd-vue"],
          "alias": {
            "@": "@wish/wd-vue/lib",
            "popper.js/dist/esm/popper": "popper.js/dist/umd/popper"
          }
        }]
      ]
    }
  }
}
