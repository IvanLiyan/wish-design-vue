    "mini-css-extract-plugin": "^0.8.0",
    "node-notifier": "^10.0.0",
    "optimize-css-assets-webpack-plugin": "^3.2.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.3.1",
