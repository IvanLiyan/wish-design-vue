{
  "name": "meimei-vue-component",
  "version": "1.0.1-alpha.0",
  "description": "vue„ÄÅcomponent",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build:all",
    "build:all": "npm run clean && npm run build:file && npm run build:component && npm run build:common && npm run build:utils && npm run build:index && npm run build:es && npm run build:css && npm run build:theme && npm run build:types && npm run copy:types",
    "clean": "rimraf lib es dist test/**/coverage",
    "build:file": "node build/bin/build-entry.js",
    "build:component": "webpack --config build/webpack.build.conf.js",
    "build:common": "webpack --config build/webpack.build-all.conf.js",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "build:index": "webpack --config build/webpack.umd.conf.js",
    "build:es": "node ./build/compiler/index.js",
    "build:css": "node build/bin/build-css.js",
    "build:theme": "gulp --gulpfile gulpfile.js build",
    "build:types": "node build/bin/build-ts.js",
    "copy:types": "cp -r types/*.d.ts lib",
    "build:deploy": "npm run clean && npm run build:css && cross-env NODE_ENV=production webpack --progress --config build/webpack.deploy.conf.js",
    "release": "node script/release.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meimei777/vue-component.git"
  },
  "keywords": [
    "vue",
    "component"
  ],
  "author": "rose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meimei777/vue-component/issues"
  },
  "homepage": "https://github.com/meimei777/vue-component#readme",
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.2.2",
    "@vue/test-utils": "^1.2.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "enquirer": "^2.3.6",
    "eslint": "^6.7.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^7.19.1",
    "execa": "^5.1.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "gulp-cssmin": "^0.2.0",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.0.0",
    "highlight.js": "^11.2.0",
    "html-webpack-plugin": "^5.3.2",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^8.3.1",
    "markdown-it-container": "^3.0.0",
    "mini-css-extract-plugin": "^2.4.2",
    "node-notifier": "^10.0.0",
    "postcss-loader": "^6.1.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "terser-webpack-plugin": "^5.2.4",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^4.1.1",
    "vue-highlight.js": "^3.1.0",
    "vue-loader": "^15.9.8",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "^3.5.2",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.58.0",
    "webpack-cli": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "vue-style-loader": "^4.1.3"
  }
}
