    "vue-template-compiler": "^2.6.14",
    "vue-text-highlight": "^2.0.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@types/node": "^16.11.7",
    "async-validator": "^1.12.2",
    "axios": "^0.26.0",
    "calendar-js": "^1.4.4",
    "dayjs": "^1.10.7",
    "debounce": "^1.2.1",
    "deepmerge": "^4.2.2",
    "js-calendar": "^1.2.3",
    "popper.js": "^1.16.0",
    "resize-observer-polyfill": "^1.5.1",
    "throttle-debounce": "^1.1.0",
    "vue-feather-icons": "^5.1.0",
    "vue-fragment": "^1.5.2",
    "vue-highlight.js": "^3.1.0"
  },
  "peerDependencies": {
    "vue": "^2.5.2"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "all",
    "tabWidth": 2,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true
  },
  "lint-staged": {
    "{src,examples,components}/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  }
}
